{
  "name": "neptune-extension",
  "description": "Jupyter neptune extension",
  "version": "0.1.0",
  "private": true,
  "license": "Apache-2.0",
  "author": "Neptune Labs",
  "main": "dist/labextension/neptune-notebook.js",
  "scripts": {
    "check-types": "tsc",
    "check-types-watch": "tsc --watch",
    "prebuild": "npm run check-types",
    "build": "npm run build-nbextension && npm run build-labextension",
    "build-nbextension": "webpack --config webpack.nbextension.config.js",
    "build-labextension": "webpack --config webpack.labextension.config.js",
    "start-nbextension": "webpack --watch --config webpack.nbextension.config.js",
    "start-labextension": "webpack --watch --config webpack.labextension.config.js",
    "install-nbextension": "jupyter nbextension install dist/nbextension/neptune-notebook.js --user && jupyter nbextension enable neptune-notebook --user",
    "install-labextension": "jupyter labextension link .",
    "lint": "eslint *.js .*.js ./config ./src"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.0",
    "@jupyterlab/apputils": "^1.0.0",
    "@jupyterlab/docregistry": "^1.0.0",
    "@jupyterlab/notebook": "^1.0.0",
    "@jupyterlab/running": "^1.0.0",
    "@jupyterlab/vdom": "^1.0.0-alpha.4",
    "@phosphor/disposable": "^1.1.2",
    "@types/lodash": "^4.14.138",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "axios": "~0.19.0",
    "lodash": "^4.0.0",
    "react": "~16.8.0",
    "react-dom": "~16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "autoprefixer": "~9.6.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.0",
    "css-loader": "~3.2.0",
    "less": "~3.10.3",
    "less-loader": "~5.0.0",
    "eslint": "^6.3.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.0.0",
    "postcss-loader": "~3.0.0",
    "style-loader": "~1.0.0",
    "typescript": "^3.6.2",
    "url-loader": "~2.1.0",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.0.0"
  },
  "jupyterlab": {
    "extension": true
  }
}
