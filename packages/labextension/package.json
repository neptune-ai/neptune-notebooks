{
  "name": "neptune-notebooks",
  "version": "0.0.11",
  "description": "A JupyterLab extension for Neptune",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/neptune-ml/neptune-notebooks",
  "bugs": {
    "url": "https://github.com/neptune-ml/neptune-notebooks/issues"
  },
  "license": "Apache-2.0",
  "author": "Neptune",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/neptune-ml/neptune-notebooks.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w",
    "dist": "rimraf dist && npm run build && mkdir dist && cd dist && npm pack .."
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.0",
    "@jupyterlab/apputils": "^1.0.0",
    "@jupyterlab/docregistry": "^1.0.0",
    "@jupyterlab/notebook": "^1.0.0",
    "@jupyterlab/running": "^1.0.0",
    "@jupyterlab/vdom": "^1.0.0-alpha.4",
    "@phosphor/disposable": "^1.1.2",
    "@types/codemirror": "0.0.74",
    "@types/react-modal": "^3.8.2",
    "codemirror": "^5.47.0",
    "react-codemirror2": "^6.0.0",
    "react-modal": "^3.8.1"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "tslint": "^5.16.0",
    "typescript": "^3.1.1",
    "typescript-tslint-plugin": "^0.3.1"
  },
  "jupyterlab": {
    "extension": true
  }
}
